{
  "classId": "idm.componet.db.Isupervisestat",
  "comName": "督办统计",
  "className": "ISuperviseStat",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "固定高度",
          "bindKey": "tableHeightFlag",
          "default": false
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "表格高度",
          "desc": "表格最大高度",
          "bindKey": "tableMaxHeight",
          "default": "calc(100%)"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "表格宽度",
          "desc": "表格最大宽度",
          "bindKey": "tableMaxWidth",
          "default": "calc(100% - 100px)"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "关键字",
          "bindKey": "keyWrod",
          "default": false
        }
      ]
    },
    {
      "type": "group",
      "text": "分页设置",
      "children": [
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "分页",
          "styleType": "radio",
          "bindKey": "papShow",
          "dictionary": [
              {
                  "label": "展示",
                  "value": true
              },
              {
                  "label": "隐藏",
                  "value": false
              }
          ],
          "default": false
        },
          {
              "type": "textarea",
              "layoutType": "inline",
              "text": "分页条数",
              "bindKey": "pageSizeOptions",
              "desc":"指定每页可以显示多少条，以逗号隔开，例如：10,20,30,40",
              "default": "10,20,30,40"
          },
          {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "默认条数",
              "bindKey": "defaultPageSize",
              "maxLength": 300,
              "minLength": 1,
              "desc":"默认的每页条数，一般指定分页条数设置的其中一个即可",
              "default": 30
          },
          {
              "type": "switch",
              "layoutType": "inline",
              "text": "禁用分页",
              "bindKey": "disabledPagination",
              "default": false
          },
          {
              "type": "switch",
              "layoutType": "inline",
              "text": "快速跳转",
              "bindKey": "showQuickJumper",
              "desc":"设置是否可以快速跳转至某页",
              "default": true
          },
          {
              "type": "switch",
              "layoutType": "inline",
              "text": "改变分页",
              "bindKey": "showSizeChanger",
              "desc":"设置是否可以改变每页的大小",
              "default": false
          },
          {
              "type": "textarea",
              "layoutType": "block",
              "text": "总量显示格式化",
              "bindKey": "showTotalFormat",
              "desc":"设置用于显示数据总量和当前数据顺序，示例：@[range[0]]-@[range[1]] of @[total] items",
              "placeholder":"示例：@[range[0]]-@[range[1]] of @[total] items",
              "default": "当前显示@[range[0]]-@[range[1]]，总共@[total]条"
          }
      ]
    },
    {
      "type": "group",
      "text": "外层样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "width",
                "width": "60px",
                "default": "100%"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "height",
                "width": "60px",
                "default": "100%"
              }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "ulbox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "20px",
              "paddingRightVal": "20px",
              "paddingBottomVal": "20px",
              "paddingLeftVal": "20px"
            }
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
                "hex": "#ffffff",
                "hex8": "#ffffff"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": ""
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "boxborder"
          }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "表格数据源参数",
          "bindKey": "handleTableParams",
          "desc": "接口参数：自定义函数参数：{_this: this, search: search, keywrodlist: keywrodlist}",
          "multiple": false
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "表格数据源",
          "bindKey": "dataSourceForm"
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "block",
          "disabled": "@[keyWrod]",
          "text": "关键字数据源",
          "desc": "关键字下拉框数据源 返回的内容格式：[{value: 1, text: '协办'}]",
          "bindKey": "keyWorddataSource"
        }
      ]
    }
  ]
}