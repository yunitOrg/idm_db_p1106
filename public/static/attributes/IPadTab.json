{
    "classId": "idm.componet.db.IPadTab",
    "comName": "轮播",
    "className": "IPadTab",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": ""
        },
        {
            "type": "group",
            "text": "基本设置",
            "children": [
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "轮播数量",
                    "bindKey": "count",
                    "default": 1,
                    "desc": ""
                }
            ]
        },
        {
            "type": "group",
            "text": "外层样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "100%"
                        }
                    ]
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "ulbox"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "背景色",
                    "bindKey": "bgColor",
                    "placeholder": "请选择颜色",
                    "default": {
                    }
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "容器阴影",
                    "bindKey": "boxShadow",
                    "default": ""
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "boxborder"
                }
            ]
        },
        {
            "type": "table",
            "text": "联动响应",
            "bindKey": "linkageEnd",
            "tabText": "响应[index]",
            "desc": "组件之间的联动作为接收方来去设置组件之间的各种联动功能，点击？查看联动设置指南",
            "children": [
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "响应标识",
                    "bindKey": "reslinkageType",
                    "desc": "联动到需求则指定数据结果将用作于控件的选项等数据，通过选择标识来过滤其他组件发送过来的消息",
                    "dictionary": [
                        {
                            "label": "联动重新加载",
                            "value": "linkageReload"
                        },
                        {
                            "label": "联动到结果",
                            "value": "linkageResult"
                        },
                        {
                            "label": "联动到需求",
                            "value": "linkageDemand"
                        },
                        {
                            "label": "自定义",
                            "value": "custom"
                        }
                    ],
                    "default": "linkageReload"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "指定标识",
                    "bindKey": "reslinkageTypeCustom",
                    "desc": "用于设置联动类型的type标识",
                    "display": "@[reslinkageType=='custom']",
                    "default": ""
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "响应条件",
                    "bindKey": "resResultRule",
                    "desc": "用于过滤消息标识或者其他消息字段来判断此次是否应用此次的消息，支持填写IDM表达式，未填写直接放行"
                },
                {
                    "type": "select",
                    "layoutType": "inline",
                    "text": "响应动作",
                    "bindKey": "resType",
                    "desc": "选择接收其他组件传值后要执行的后续动作，也就是选择接收到的值的作用是什么",
                    "dictionary": [
                        {
                            "label": "无",
                            "value": "none"
                        },
                        {
                            "label": "自定义函数",
                            "value": "customFun"
                        }
                    ],
                    "default": "none"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "自定义函数",
                    "bindKey": "resFunction",
                    "desc": "其他组件传值后会把值传递到此方法并且调用，然后自定义函数返回结果，接收参数{messageObject：联动的消息,moduleObject:组件对象}",
                    "display": "@[resType=='customFun']"
                }
            ],
            "default": []
        }
    ]
}