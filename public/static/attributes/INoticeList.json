{
    "classId": "idm.componet.db.INoticeList",
    "comName": "通知列表",
    "className": "INoticeList",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "group",
            "text": "外层样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "100%"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "100%"
                        }
                    ]
                },
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "ulbox"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "背景色",
                    "bindKey": "bgColor",
                    "placeholder": "请选择颜色",
                    "default": {
                        "hex": "#ffffff",
                        "hex8": "#ffffff"
                    }
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "容器阴影",
                    "bindKey": "boxShadow",
                    "default": ""
                },
                {
                    "type": "border",
                    "layoutType": "block",
                    "text": "边框",
                    "bindKey": "boxborder"
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "自定义请求函数",
                    "bindKey": "handleRequestFunc",
                    "desc": "接口参数：自定义请求函数：{record} 返回：Promise<Array>",
                    "multiple": false
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "接口返回数据处理函数",
                    "bindKey": "hanldeInterfaceFunc",
                    "desc": "接口参数：自定义函数参数：{_this: this, data: data}",
                    "multiple": false
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "操作项函数",
                    "bindKey": "handleActionFunc",
                    "desc": "接口参数：自定义函数参数：{_this: this, option: option}",
                    "multiple": false
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "跳转函数",
                    "bindKey": "handleChildFunc",
                    "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
                    "multiple": false
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "附件跳转函数",
                    "bindKey": "handleFileFunc",
                    "desc": "接口参数：自定义函数参数：{_this: this, item: item}",
                    "multiple": false
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "历史弹窗函数",
                    "bindKey": "handleHistoryFunc",
                    "desc": "接口参数：自定义函数参数：{record}",
                    "multiple": false
                }
            ]
        }
    ]
}